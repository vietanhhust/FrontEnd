{
    "files.exclude": {
        "**/.git*": true,
        "**/npm-debug*": true,
        "**/*.js.map": true,
        "**/*.js": {"when": "$(basename).ts"},
        "node_modules/": true,
        "bower_components/": true,
        "dist/": true,
        "e2e/": true,
        "iis/": true,
        "src/assets/": true,
        "src/*.json": true,
        "src/browserslist": true,
        "src/*.js": true,

        
        "*.editorconfig": true,
        "browserslist": true,
        "*.js": true,
        ".vscode": true,
    },
    "explorer.autoReveal": true
}